pipeline{

    agent any
    
    environment {
        dotnet = "C:\\Program Files\\dotnet"
    }

    triggers{
        pollSCM 'H * * * *'
    }

    stages {
        
        stage("Restore"){
            
            steps{
                echo 'Just fine'
                sh 'dotnet restore Project-Plutus.sln'
                echo 'Just fine 2'
            }
        
        }
      
    }
    

}