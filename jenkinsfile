pipeline{

    agent any

    stages {
        
        stage("Restore"){
            
            steps{
                bat 'dotnet restore'
                bat 'dotnet build'
            }
        
        }
        stage("Test"){
            
            steps{
                bat 'cd Plutus-Tests'
                bat 'dotnet test'
            }
        
        }
      
    }
    

}